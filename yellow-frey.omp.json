{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "╭─"
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#000000",
          "background": "#E77728",
          "leading_diamond": "\ue0b6",
          "template": "\uf4ff {{ .UserName }} {{ if .SSHSession }}\udb82\udca9{{else}}\udb80\udf79{{ end }} {{ .HostName }}",
          "trailing_diamond": "\ue0b4"
        },
        {
          "type": "text",
          "style": "plain",
          "template": "─"
        },
        {
          "type": "text",
          "style": "plain",
          "template": " "
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#000000",
          "background": "#EDB230",
          "leading_diamond": "\ue0b6",
          "template": "\udb85\udc46 {{ .CurrentDate | date .Format }}",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "time_format": "2006-01-02 15:04:05"
          }
        },
        {
          "foreground": "#F3C267",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044{{ if gt .Working.Modified 0 }} <#FF9248>~{{ .Working.Modified }}</>{{ end }}{{ if gt .Working.Deleted 0 }} <#FF5555>-{{ .Working.Deleted }}</>{{ end }}{{ if gt .Working.Untracked 0 }} <#5EAEFF>?{{ .Working.Untracked }}</>{{ end }}{{ if gt .Working.Added 0 }} <#98C379>+{{ .Working.Added }}</>{{ end }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046{{ if gt .Staging.Modified 0 }} <#FF9248>~{{ .Staging.Modified }}</>{{ end }}{{ if gt .Staging.Deleted 0 }} <#FF5555>-{{ .Staging.Deleted }}</>{{ end }}{{ if gt .Staging.Added 0 }} <#98C379>+{{ .Staging.Added }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "type": "git"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "#906cff",
          "template": " \ue73c {{ .Full }} {{ .Venv }}"
        }
      ]
    },
    {
      "alignment": "right",
      "type": "prompt",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#b8ff75",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ],
          "template": " x{{ reason .Code }}"
        },
        {
          "foreground": "#b8ff75",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ],
          "properties": {
            "style": "roundrock",
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ]
    },
    {
      "alignment": "left",
      "type": "prompt",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "│ "
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#FFDDA1",
          "background": "transparent",
          "leading_diamond": "\ue0b6",
          "template": "\ue5ff {{ .Path }}",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "style": "full"
          }
        }
      ]
    },
    {
      "alignment": "left",
      "type": "prompt",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "╰─\ueb70"
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "$ "
  },
  "final_space": true,
  "version": 2
}